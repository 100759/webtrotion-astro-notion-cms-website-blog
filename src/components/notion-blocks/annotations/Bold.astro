---
import { RichText } from "../../../lib/interfaces.ts";

export interface Props {
	richText: RichText;
}

const { richText } = Astro.props;
---

{
	/* prettier-ignore */
	richText.Annotation.Bold ? (
  <strong class="font-semibold text-gray-900 dark:text-white"><slot /></strong>
  ) : (
    <slot />
  )
}
